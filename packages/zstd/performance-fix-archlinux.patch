From e61036aee7c1868cfd2c04028aaca0c1ebb2f07f Mon Sep 17 00:00:00 2001
From: Peter Jung <admin@ptr1337.dev>
Date: Sat, 18 Jun 2022 14:45:37 +0200
Subject: [PATCH] zstd-cmake performance fix archlinux

---
 build/cmake/CMakeModules/AddZstdCompilationFlags.cmake | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/build/cmake/CMakeModules/AddZstdCompilationFlags.cmake b/build/cmake/CMakeModules/AddZstdCompilationFlags.cmake
index e23b9d60..7cbdcd2f 100644
--- a/build/cmake/CMakeModules/AddZstdCompilationFlags.cmake
+++ b/build/cmake/CMakeModules/AddZstdCompilationFlags.cmake
@@ -25,7 +25,7 @@ macro(ADD_ZSTD_COMPILATION_FLAGS)
         #Set c++11 by default
         EnableCompilerFlag("-std=c++11" false true)
         #Set c99 by default
-        EnableCompilerFlag("-std=c99" true false)
+#        EnableCompilerFlag("-std=c99" true false)
         if (CMAKE_CXX_COMPILER_ID MATCHES "Clang" AND MSVC)
             # clang-cl normally maps -Wall to -Weverything.
             EnableCompilerFlag("/clang:-Wall" true true)
